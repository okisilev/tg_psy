            order_id=f"women_club_{user_id}_{int(time.time())}",
            amount=5000,  # 50 —Ä—É–±–ª–µ–π –≤ –∫–æ–ø–µ–π–∫–∞—Ö
            description="–î–æ—Å—Ç—É–ø –∫ –æ–±—É—á–∞—é—â–∏–º –º–∞—Ç–µ—Ä–∏–∞–ª–∞–º",
            user_id=user_id,
